{
  "dots": {
    "interval": 50,
    "frames": ["⠋", "⠙", "⠹", "⠸", "⠼", "⠴", "⠦", "⠧", "⠇", "⠏"]
  },
  "dashes": {
    "interval": 100,
    "frames": ["-", "_"]
  },
  "slashes": {
    "interval": 100,
    "frames": ["/", "\\"]
  },
  "loadingBar": {
    "interval": 80,
    "frames": []
  },
  "backwardsLoadingBar": {
    "interval": 80,
    "frames": ["[      ]", "[     =]", "[    ==]", "[   == ]", "[  ==  ]", "[ ==   ]", "[==    ]", "[=     ]"]
  },
  "twoWayLoadingBar": {
    "interval": 80,
    "frames": ["[      ]", "[=     ]", "[==    ]", "[ ==   ]", "[  ==  ]", "[   == ]", "[    ==]", "[     =]", "[      ]", "[     =]", "[    ==]", "[   == ]", "[  ==  ]", "[ ==   ]", "[==    ]", "[=     ]"]
  },
  "loadingDots": {
    "interval": 80,
    "frames": ["     ", ".    ", "..   ", " ..  ", "  .. ", "   ..", "    ."]
  },
  "backwardsLoadingDots": {
    "interval": 80,
    "frames": ["     ", "    .", "   ..", "  .. ", " ..  ", "..   ", ".    "]
  }
}
